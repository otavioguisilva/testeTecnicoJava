<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="270.0" prefWidth="380.0" styleClass="window" stylesheets="@../css/Geral.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.rockeit.agendamentoconsultas.controllers.TelaAlteracaoSenha">
   <top>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="510.0">
