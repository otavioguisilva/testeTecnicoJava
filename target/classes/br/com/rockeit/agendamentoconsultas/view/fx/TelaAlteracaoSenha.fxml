            <HBox>
               <children>
                  <Button fx:id="btnConfirm" mnemonicParsing="false" onAction="#onBtnConfirmAction" prefHeight="50.0" prefWidth="120.0" text="Confirmar" HBox.hgrow="ALWAYS">
