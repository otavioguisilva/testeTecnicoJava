                  </HBox>
                  <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                     <children>
                        <PasswordField fx:id="txtSenhaNova" prefHeight="30.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
